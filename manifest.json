{
  "name": "GameStoreBlacklist",
  "manifest_version": 3,
  "version": "2.0.0",
  "description": "",
  "action": {
    "default_popup": "src/component/popup/popup.html",
    "default_icon": "image/icon.png"
  },
  "background": {
    "service_worker": "src/component/background/background.js"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "css": [
        "src/component/web/greenmangaming/mainPage/style.css"
      ],
      "js": [
        "src/component/web/greenmangaming/mainPage/content.js"
      ],
      "matches": [
        "https://www.greenmangaming.com/",
        "https://www.greenmangaming.com/de/",
        "https://www.greenmangaming.com/fr/",
        "https://www.greenmangaming.com/es/",
        "https://www.greenmangaming.com/pt/",
        "https://www.greenmangaming.com/zh/",
        "https://www.greenmangaming.com/ja/",
        "https://www.greenmangaming.com/ko/",
        "https://www.greenmangaming.com/ru/",
        "https://www.greenmangaming.com/tr/"
      ]
    },
    {
      "css": [
        "src/component/web/greenmangaming/sales/style.css"
      ],
      "js": [
        "src/component/web/greenmangaming/sales/content.js"
      ],
      "matches": [
        "https://www.greenmangaming.com/sales/*",
        "https://www.greenmangaming.com/*/sales/*",
        "https://www.greenmangaming.com/hot-deals/*",
        "https://www.greenmangaming.com/*/hot-deals/*"
      ]
    },
    {
      "css": [
        "src/component/web/greenmangaming/comingSoon/style.css"
      ],
      "js": [
        "src/component/web/greenmangaming/comingSoon/content.js"
      ],
      "matches": [
        "https://www.greenmangaming.com/all-games/",
        "https://www.greenmangaming.com/*/all-games/",
        "https://www.greenmangaming.com/coming-soon/",
        "https://www.greenmangaming.com/*/coming-soon/",
        "https://www.greenmangaming.com/top-upcoming-games/",
        "https://www.greenmangaming.com/*/top-upcoming-games/",
        "https://www.greenmangaming.com/new-gmg/",
        "https://www.greenmangaming.com/*/new-gmg/"
      ]
    },
    {
      "css": [
        "src/component/web/greenmangaming/search/style.css"
      ],
      "js": [
        "src/component/web/greenmangaming/search/content.js"
      ],
      "matches": [
        "https://www.greenmangaming.com/all-games/*",
        "https://www.greenmangaming.com/*/all-games/*",
        "https://www.greenmangaming.com/top-upcoming-games/pre-purchase/",
        "https://www.greenmangaming.com/*/top-upcoming-games/pre-purchase/",
        "https://www.greenmangaming.com/new-gmg/all-results/",
        "https://www.greenmangaming.com/new-gmg/*/all-results/",
        "https://www.greenmangaming.com/search?*",
        "https://www.greenmangaming.com/*/search?*"
      ]
    },
    {
      "css": [
        "src/component/web/gamesplanet/style.css"
      ],
      "js": [
        "src/component/web/gamesplanet/content.js"
      ],
      "matches": [
        "https://*.gamesplanet.com/*"
      ]
    },
    {
      "css": [
        "src/component/web/fanatical/style.css"
      ],
      "js": [
        "src/component/web/fanatical/content.js"
      ],
      "matches": [
        "https://www.fanatical.com/*"
      ]
    },
    {
      "css": [
        "src/component/web/yuplay/mainPage/style.css"
      ],
      "js": [
        "src/component/web/yuplay/mainPage/content.js"
      ],
      "matches": [
        "https://www.yuplay.com/"
      ]
    },
    {
      "css": [
        "src/component/web/yuplay/product/style.css"
      ],
      "js": [
        "src/component/web/yuplay/product/content.js"
      ],
      "matches": [
        "https://www.yuplay.com/product/*"
      ]
    },
    {
      "css": [
        "src/component/web/yuplay/search/style.css"
      ],
      "js": [
        "src/component/web/yuplay/search/content.js"
      ],
      "matches": [
        "https://www.yuplay.com/products/*"
      ]
    },
    {
      "css": [
        "src/component/web/gamivo/style.css"
      ],
      "js": [
        "src/component/web/gamivo/content.js"
      ],
      "matches": [
        "https://www.gamivo.com/*"
      ]
    },
    {
      "css": [
        "src/component/web/ggdeals/product/style.css"
      ],
      "js": [
        "src/component/web/ggdeals/product/content.js"
      ],
      "matches": [
        "https://gg.deals/game/*",
        "https://gg.deals/dlc/*",
        "https://gg.deals/pack/*"
      ]
    },
    {
      "css": [
        "src/component/web/ggdeals/deals/style.css"
      ],
      "js": [
        "src/component/web/ggdeals/deals/content.js"
      ],
      "matches": [
        "https://gg.deals/deals/*"
      ]
    },
    {
      "css": [
        "src/component/web/ggdeals/games/style.css"
      ],
      "js": [
        "src/component/web/ggdeals/games/content.js"
      ],
      "matches": [
        "https://gg.deals/games/*"
      ]
    },
    {
      "css": [
        "src/component/web/humblebundle/mainPage/style.css"
      ],
      "js": [
        "src/component/web/humblebundle/mainPage/content.js"
      ],
      "matches": [
        "https://www.humblebundle.com/"
      ]
    },
    {
      "css": [
        "src/component/web/humblebundle/product/style.css"
      ],
      "js": [
        "src/component/web/humblebundle/product/content.js"
      ],
      "matches": [
        "https://www.humblebundle.com/store/*"
      ]
    },
    {
      "css": [
        "src/component/web/humblebundle/search/style.css"
      ],
      "js": [
        "src/component/web/humblebundle/search/content.js"
      ],
      "matches": [
        "https://www.humblebundle.com/store/search?*",
        "https://www.humblebundle.com/store/c/*"
      ]
    },
    {
      "css": [
        "src/component/web/humblebundle/promo/style.css"
      ],
      "js": [
        "src/component/web/humblebundle/promo/content.js"
      ],
      "matches": [
        "https://www.humblebundle.com/store/promo/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "image/checkbox_across_enabled.png",
        "image/checkbox_across_disabled.png",
        "image/checkbox_tick_enabled.png",
        "image/checkbox_tick_disabled.png"
      ],
      "matches": [
        "https://www.yuplay.com/*",
        "https://www.gamivo.com/*",
        "https://gg.deals/*",
        "https://www.humblebundle.com/*",
        "https://www.fanatical.com/*",
        "https://*.gamesplanet.com/*",
        "https://www.greenmangaming.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://www.yuplay.com/*",
    "https://www.gamivo.com/*",
    "https://gg.deals/*",
    "https://www.humblebundle.com/*",
    "https://www.fanatical.com/*",
    "https://*.gamesplanet.com/*",
    "https://www.greenmangaming.com/*"
  ]
}