{
  "name": "GameStoreBlacklist",
  "manifest_version": 3,
  "version": "1.9.3",
  "description": "",
  "action": {
    "default_popup": "src/component/popup/popup.html",
    "default_icon": "image/icon.png"
  },
  "background": {
    "service_worker": "src/component/background/background.js"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "css": [
        "src/component/web/greenmangaming/sales/style.css"
      ],
      "js": [
        "src/component/web/greenmangaming/sales/content.js"
      ],
      "matches": [
        "https://www.greenmangaming.com/*/sales*"
      ]
    },
    {
      "css": [
        "src/component/web/greenmangaming/comingSoon/style.css"
      ],
      "js": [
        "src/component/web/greenmangaming/comingSoon/content.js"
      ],
      "matches": [
        "https://www.greenmangaming.com/*/coming-soon*"
      ]
    },
    {
      "css": [
        "src/component/web/gamesplanet/search/style.css"
      ],
      "js": [
        "src/component/web/gamesplanet/search/content.js"
      ],
      "matches": [
        "https://*.gamesplanet.com/search?*"
      ]
    },
    {
      "css": [
        "src/component/web/fanatical/general/style.css"
      ],
      "js": [
        "src/component/web/fanatical/general/content.js"
      ],
      "matches": [
        "https://www.fanatical.com/*/",
        "https://www.fanatical.com/*/on-sale",
        "https://www.fanatical.com/*/?langRedirect=*"
      ]
    },
    {
      "css": [
        "src/component/web/fanatical/product/style.css"
      ],
      "js": [
        "src/component/web/fanatical/product/content.js"
      ],
      "matches": [
        "https://www.fanatical.com/*/game/*"
      ]
    },
    {
      "css": [
        "src/component/web/fanatical/search/style.css"
      ],
      "js": [
        "src/component/web/fanatical/search/content.js"
      ],
      "matches": [
        "https://www.fanatical.com/*/search*"
      ]
    },
    {
      "css": [
        "src/component/web/yuplay/product/style.css"
      ],
      "js": [
        "src/component/web/yuplay/product/content.js"
      ],
      "matches": [
        "https://www.yuplay.com/",
        "https://www.yuplay.com/product/*"
      ]
    },
    {
      "css": [
        "src/component/web/yuplay/search/style.css"
      ],
      "js": [
        "src/component/web/yuplay/search/content.js"
      ],
      "matches": [
        "https://www.yuplay.com/products/*"
      ]
    },
    {
      "css": [
        "src/component/web/gamivo/product/style.css"
      ],
      "js": [
        "src/component/web/gamivo/product/content.js"
      ],
      "matches": [
        "https://www.gamivo.com/product/*"
      ]
    },
    {
      "css": [
        "src/component/web/gamivo/search/style.css"
      ],
      "js": [
        "src/component/web/gamivo/search/content.js"
      ],
      "matches": [
        "https://www.gamivo.com/*"
      ]
    },
    {
      "css": [
        "src/component/web/ggdeals/product/style.css"
      ],
      "js": [
        "src/component/web/ggdeals/product/content.js"
      ],
      "matches": [
        "https://gg.deals/game/*",
        "https://gg.deals/dlc/*",
        "https://gg.deals/pack/*"
      ]
    },
    {
      "css": [
        "src/component/web/ggdeals/search/style.css"
      ],
      "js": [
        "src/component/web/ggdeals/search/content.js"
      ],
      "matches": [
        "https://gg.deals/deals/*"
      ]
    },
    {
      "css": [
        "src/component/web/humblebundle/product/style.css"
      ],
      "js": [
        "src/component/web/humblebundle/product/content.js"
      ],
      "matches": [
        "https://www.humblebundle.com/store/*"
      ]
    },
    {
      "css": [
        "src/component/web/humblebundle/search/style.css"
      ],
      "js": [
        "src/component/web/humblebundle/search/content.js"
      ],
      "matches": [
        "https://www.humblebundle.com/store/search?*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "image/checkbox_across_enabled.png",
        "image/checkbox_across_disabled.png",
        "image/checkbox_tick_enabled.png",
        "image/checkbox_tick_disabled.png"
      ],
      "matches": [
        "https://www.yuplay.com/*",
        "https://www.gamivo.com/*",
        "https://gg.deals/*",
        "https://www.humblebundle.com/*",
        "https://www.fanatical.com/*",
        "https://*.gamesplanet.com/*",
        "https://www.greenmangaming.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://www.yuplay.com/*",
    "https://www.gamivo.com/*",
    "https://gg.deals/*",
    "https://www.humblebundle.com/*",
    "https://www.fanatical.com/*",
    "https://*.gamesplanet.com/*",
    "https://www.greenmangaming.com/*"
  ]
}